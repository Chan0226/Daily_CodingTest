# 횟수
N = int(input())
# 시간 입력 리스트
time = []
# 시간 인풋
for _ in range(N):
    s, e = map(int,input().split())
    time.append([s,e])
# 시간을 시작 순서대로 정렬하기
time = sorted(time, key = lambda x:x[0])
# 시간을 끝나는 순서대로 정렬하기
time = sorted(time, key = lambda x:x[1])
# 끝나는 시간
last = 0
# 횟수
count = 0
# time 리스트의 시작과 끝 시간 반복
for i,j in time:
    # 끝나는 시간보다 i의 시작시간이 크면
    if i>=last:
        # 횟수 1회 증가
        count +=1
        # 마지막 시간은 j로 되야함
        last = j
print(count)
