function solution(n) {
    var answer = [];

    for(let i = 0; i < n; i++){
        const cells = [];
        for(let j = 0; j < n; j++){
            if(i==j){
                cells.push(1);                
            }else{
                 cells.push(0);   
            }
        }
        answer.push(cells);
    }
    return answer;
}
