###1) function solution(arr, queries) {
    let s = 0;
    let e = 0;
    let k = 0;
    queries.forEach(item => {
        s = item[0]
        e = item[1]
        k = item[2]
        for(let i=s; i<=e; i++){
            if(i % k == 0){
                arr[i] += 1
            }
        }
    })
    return arr;
}

###2) 
