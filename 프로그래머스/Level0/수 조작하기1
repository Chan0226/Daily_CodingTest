function solution(n, control) {
    var answer = 0;
    const controlList = [...control]
    for(let item of controlList){
        if(item == 'w'){
            n += 1
        }else if(item == 's'){
            n -= 1
        }else if(item == 'd'){
            n += 10
        }else if(item == 'a'){
            n -= 10
        }
    }
    answer = n
    return answer;
}
